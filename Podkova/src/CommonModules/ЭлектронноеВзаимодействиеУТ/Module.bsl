	КонецЕсли;
	
	Запрос = Новый Запрос;
	Запрос.Текст = ТекстЗапроса;
	Запрос.УстановитьПараметр("Ссылка", ДокументСсылка);
	
	Возврат Запрос.Выполнить().Выгрузить();
	
КонецФункции

Функция ПолучитьКоэффициентРНПТ(Документ, ТекущаяСтрока, НомерГТД)	
	
	ПараметрыПолученияКоэффициентаРНПТ = УчетПрослеживаемыхТоваровКлиентСерверЛокализация.ПараметрыПолученияКоэффициентаРНПТ(
											Документ,
											"Склад",
											Ложь);
											
	КэшированныеЗначения = ОбработкаТабличнойЧастиКлиентСервер.ПолучитьСтруктуруКэшируемыеЗначения();
	
	ТекущаяСтрокаДляОтбора = Новый Структура;
	ТекущаяСтрокаДляОтбора.Вставить("Номенклатура",		ТекущаяСтрока.Номенклатура);
	ТекущаяСтрокаДляОтбора.Вставить("Характеристика",	ТекущаяСтрока.Характеристика);
	ТекущаяСтрокаДляОтбора.Вставить("НомерГТД",			НомерГТД);
	
	Коэффициент = УчетПрослеживаемыхТоваровКлиентСерверЛокализация.ПолучитьКоэффициентРНПТ(ПараметрыПолученияКоэффициентаРНПТ,
				ТекущаяСтрокаДляОтбора,
				КэшированныеЗначения);	
	
	Возврат Коэффициент;
	
КонецФункции

